{
  "name": "Counter",
  "description": "Counter for manga website thing",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "media/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chainsawmann.com/manga/chainsaw-man-chapter-*/*"],
      "js": ["content.js"]
    }
  ]  
}